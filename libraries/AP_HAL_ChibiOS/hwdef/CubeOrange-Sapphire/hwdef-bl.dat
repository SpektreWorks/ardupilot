# hw definition file for processing by chibios_hwdef.py
# for H743 bootloader

include ../CubeOrange/hwdef-bl.dat

undef USB_STRING_PRODUCT
undef USB_STRING_MANUFACTURER
undef APJ_BOARD_ID

# USB setup
USB_STRING_MANUFACTURER "CubePilot"
USB_STRING_PRODUCT "CubeOrange-BL"

undef SERIAL_ORDER
SERIAL_ORDER OTG1

# these are power/starter pins, drive them low
PE9  AUX4 OUTPUT HIGH
PD13 AUX5 OUTPUT LOW
PD14 AUX6 OUTPUT LOW

APJ_BOARD_ID 6099
